<%= form_with model: @area, local:true do |f| %>
    <div>
        <%= f.label :name, "Name of region" %>
        <%= f.text_field :name %> 
    </div>
    <div class="form-group"> 
        <%= f.label :file, "Upload CSV File" %>
        <%= f.file_field :file, class: 'form-control-file', required: true %>
    </div>
    <%= f.button :submit %>
<% end %>

	
